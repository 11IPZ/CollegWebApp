@model IEnumerable<CollegWebApp.Domain.Models.User>
 
<h2>Список користувачів</h2>
<table class="table">
    @if (Model != null)
    {
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email @user.UserSurname @user.Name</td>
                @*<td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступу</a>
                    </td>*@
            </tr>
        }

    }
    else
    {
        <h3>Список пустий</h3>
    }
</table>